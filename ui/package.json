    "url": "git+https://github.com/mvromer/EE360T.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "Jeff Ng & Michael Romer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvromer/EE360T.git"
  },
  "eslintConfig": {
