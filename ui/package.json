    "style-loader": "^0.18.2",
    "webpack": "^4.16.3",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "he": "^1.1.1",
    "split.js": "^1.3.5"
